<%- include('../pembungkus/head.ejs') %>
<nav class="navbar navbar-expand-lg bg-body-tertiary ungu">
    <div class="container-fluid">
      <a class="navbar-brand" href="#foto">KOST URIP</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav">
          <a class="nav-link" href="#fasilitas">FASILITAS</a>
          <a class="nav-link" href="#kontak">KONTAK</a>
          <a class="nav-link" href="#alamat">ALAMAT</a>
        </div>
      </div>
    </div>
</nav>

<body>
    <!-- foto kost -->
    <section class="ungu carousel-padd">
        <div id="foto" class="carousel slide" data-bs-ride="true">
            <div class="carousel-inner">
                <div class="carousel-item active">
                  <img src="https://drive.google.com/uc?export=view&id=1NepIAcYxGNWQYsWTo--D8ozju_0zeA1b" class="d-block" alt="...">
                </div>
                <div class="carousel-item">
                  <img src="https://shared.cdn.smp.schibsted.com/v2/images/782003a2-5f1d-4ff3-95b1-1c38c4d1d53e?fit=crop&format=auto&h=847&w=1505&s=58f5960b58ab76b277b38eb62877c6f0c888cd60" class="d-block" alt="...">
                </div>
                
              </div>
              <button class="carousel-control-prev" type="button" data-bs-target="#foto" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
              </button>
              <button class="carousel-control-next" type="button" data-bs-target="#foto" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
              </button>
        </div>
    </section>
    <!-- fasilitas -->
    <section class="putih">
        <div id="fasilitas">
            <h2>Fasilitas</h2>
            <div class="fasilitas row d-flex justify-content-center">
                <div class="box col col-lg-2">
                    <ul>
                        <% for( let index = 0; index < fasilitas1.length; index++ ) { %>
                            <li><%= fasilitas1[index] %></li>
                        <% } %>
                    </ul>
                </div>
                <div class="box col col-lg-2">
                    <ul>
                        <% for( let index = 0; index < fasilitas2.length; index++ ) { %>
                            <li><%= fasilitas2[index] %></li>
                        <% } %>
                    </ul>
                </div>
            </div>
        </div>
    </section>
    <section class="ungu">
        <div id="kontak">
            <h2>Kontak</h2>
            <div class="kontak row d-flex justify-content-center">
                <div class="box col col-lg-3.3">
                    <h4>Email</h4>
                    <p><%= email %></p>
                </div>
                <div class="box col col-lg-3.3">
                    <h4>WhatsApp</h4>
                    <p><%= whatsApp %></p>
                </div>
                <div class="box col col-lg-3.3">
                    <h4>Telepon</h4>
                    <p><%= telepon %></p>
                </div>
            </div>
        </div>
    </section>
    <section class="putih">
        <div id="alamat">
            <h2>Alamat</h2>
            <p>Jl. Jendral Urip Tengah, Kec. Pontianak Kota, Kota Pontianak, Kalimantan Barat 78243</p>
            <div class="d-flex justify-content-center">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d997.4544817085786!2d109.33517254810737!3d-0.02544710121774068!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x2e1d5859a36fd2a3%3A0x82a9b405e2718326!2sJl.%20Jendral%20Urip%2C%20Tengah%2C%20Kec.%20Pontianak%20Kota%2C%20Kota%20Pontianak%2C%20Kalimantan%20Barat%2078243!5e0!3m2!1sid!2sid!4v1679322665277!5m2!1sid!2sid" width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>
        </div>
    </section>
</body>

